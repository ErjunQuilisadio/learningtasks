*{
	margin: 0;
	padding: 0;
	font-family: Open Sans;
}

header{
	background-image: url(CLE1/CLE1MT/bg.jpg);
	height: 100vh;
	background-size: cover;
	background-position: center;
}
ul{
	float: right;
	list-style-type: none;
	margin-top: 18px;
	margin-right: 32%;
}
ul li{
	display: inline-block;
}
	
ul li a{
    text-decoration: none;
    color: #000;
    padding: 7px 10px;
    border: 1px solid transparent;
    transition: 0.6s ease;
}
ul li a:hover{
	background-color: #008000;
	color: #fff;
}
.title{
	position: absolute;
	top: 30%;
	left: 50%;
	transform: translate(-50%,-50%);
}
#description p{
	position: absolute;
	font-size: 20px;
	top: 40%;
	left: 5%;
	align-content: center;
	margin: 0 auto;
	margin-left: 25%;
	margin-right: 25%;
}

